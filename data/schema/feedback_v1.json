{
    "$id": "https://edc.ons.gov.uk/schemas/feedback_v1.json",
    "$schema": "http://json-schema.org/draft-06/schema",
    "properties": {
        "collection": {
            "properties": {
                "exercise_sid": {
                    "type": "string"
                },
                "instrument_id": {
                    "type": "string"
                },
                "period": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "data": {
            "properties": {
                "email": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "metadata": {
            "properties": {
                "ru_ref": {
                    "type": "string"
                },
                "user_id": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "origin": {
            "pattern": "^uk.gov.ons.edc.eq$",
            "type": "string"
        },
        "submitted_at": {
            "description": "Time at which survey feedback was submitted.",
            "format": "date-time",
            "type": "string"
        },
        "survey_id": {
            "description": "The ONS id of the survey derived from the inquiry code already in use for that survey.",
            "pattern": "^[0-9]{3}$",
            "type": "string"
        },
        "tx_id": {
            "description": "Survey transaction identifier. This will be the same for all feedback during a survey session.",
            "pattern": "^[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12}$",
            "type": "string"
        },
        "type": {
            "pattern": "^uk.gov.ons.edc.eq:feedback$",
            "type": "string"
        },
        "version": {
            "pattern": "^0.0.1$",
            "type": "string"
        }
    },
    "type": "object"
}
